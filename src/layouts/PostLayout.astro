---
import BasicLayout from './BasicLayout.astro';
import Post from '../components/Post.astro';

const postMarkdown = Astro.self;
const { frontmatter, url } = Astro.props;
const { title, publishDate } = frontmatter;
---
<BasicLayout pageTitle={title}>
  <Post url={url} title={title} publishDate={publishDate}>
    <slot />
  </Post>
</BasicLayout>
